<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:label="@string/app_name"
        android:icon="@mipmap/maze_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Larble"
        tools:targetApi="31"
        tools:ignore="LockedOrientationActivity">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.LOGIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <activity android:name=".MenuActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MENU" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <activity android:name=".SignUpActivity"
            android:screenOrientation="portrait"
            android:exported="true">

           <intent-filter>
               <action android:name="android.intenr.action.SIGNUP" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

           </intent-filter>
        </activity>
        <activity android:name=".SinglePlayerActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.SINGLEPLAYER" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".MultiPlayerActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.MULTIPLAYER" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".NewGameActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.NEWGAME" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".ExistedGameActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.GAME" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".LeaderboardActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.SCOREBOARD" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>


        <activity android:name=".MultiPlayerGameActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

            <intent-filter>
                <action android:name="android.intenr.action.MULTIPLAYERGAME" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.SETTINGS" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".AccountActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.Account" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity android:name=".BallActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intenr.action.BALL" />
                <category android:name="android.intent.cateogyr.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".GameOverActivity"
            android:screenOrientation="portrait"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.GameOver" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>