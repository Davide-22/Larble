                <action android:name="android.intenr.action.SINGLEPLAYERGAME" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".MultiPlayerGameActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.MULTIPLAYERGAME" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intenr.action.SETTINGS" />

                <category android:name="android.intent.cateogyr.LAUNCHER" />

            </intent-filter>
        </activity>
    </application>

</manifest>